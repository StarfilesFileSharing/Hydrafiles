{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES6",
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"moduleResolution": "node",
		"outDir": "./dist",
		"rootDir": "./src"
	}
}
